modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "GADIApp-backend"

[[workflows.workflow]]
name = "GADIApp-backend"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "GADI_APP_ENV=local uvicorn app.main:app --reload --host 0.0.0.0 --port 8000"
waitForPort = 8000

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 33497
externalPort = 80
